#CREATEING  PLAYER TABLE 

CREATE TABLE PLAYER(
TEAM_ID REFERENCES TEAM,PLAYER_ID VARCHAR2(20),PLAYER_NAME VARCHAR2(50),PLAYER_ROLE VARCHAR2(30),BATTING_STYLE VARCHAR2(50));

